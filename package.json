{
  "name": "swarm-city",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "https://github.com/swarmcity/SwarmCitySite.git",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "buffer": "^5.1.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-html": "^3.2.2",
    "eth-crypto": "^1.2.3",
    "ethereumjs-util": "^5.2.0",
    "polymer-cli": "^1.7.7",
    "qrcode": "^0.9.0",
    "scryptsy": "^2.0.0",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "uuid": "^3.3.2",
    "web3-eth-contract": "^1.0.0-beta.35"
  },
  "scripts": {
    "lint": "npm run lint:javascript",
    "lint:javascript": "eslint . --ext js,html",
    "test": "wct",
    "start": "node server.js",
    "test:integration": "polymer build",
    "webpack": "webpack",
    "deploy": "node --max-old-space-size=4096 ./node_modules/.bin/polymer build && npm run push",
    "push": "git add build/es6-bundled -f && git commit -m 'deploying to branch' && git subtree push --prefix build/es6-bundled origin gh-pages",
    "build": "node --max-old-space-size=4096 ./node_modules/.bin/polymer build && npm run move && npm run delbuild",
    "move": "mv ./build/es6-bundled/* ./ && npm run delbuild",
    "delbuild": "if [ -d 'build/'; then rm -Rf build/; fi",
    "serve": "polymer serve"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "bower": "^1.8.4",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-tx": "^1.3.7",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "moment": "^2.22.2",
    "prpl-server": "^0.7.0",
    "redux": "^3.7.2",
    "web3-utils": "^1.0.0-beta.35",
    "webpack": "^3.12.0"
  }
}
